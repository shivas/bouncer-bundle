{
    "name": "serendipity_hq/aws-ses-monitor-bundle",
    "type": "symfony-bundle",
    "description": "Symfony 2.8 bundle to monitor AWS SES bounces and complaints. Includes SwiftMailer filters.",
    "keywords": ["aws", "ses", "sns", "bounce", "complaints", "blacklist", "swiftmailer"],
    "homepage": "https://github.com/Aerendir/aws-ses-bouncer-bundle",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/Aerendir/aws-ses-monitor-bundle/issues"
    },
    "authors": [
        {
            "name": "Audrius Karabanovas",
            "email": "audrius@karabanovas.net"
        },
        {
            "name": "Adamo Aerendir Crespi",
            "email": "hello@aerendir.me"
        }
    ],
    "require": {
        "php": "^5.6|^7.0",
        "aws/aws-sdk-php": "^3.2",
        "aws/aws-php-sns-message-validator": "*",
        "doctrine/orm": "~2.5",
        "symfony/framework-bundle": "~2",
        "symfony/swiftmailer-bundle": "~2.3"
    },
    "require-dev": {
        "codeclimate/php-test-reporter": "dev-master",
        "doctrine/doctrine-bundle": "*",
        "doctrine/common": "~2.4",
        "symfony/yaml": "~2.5",
	    "symfony/console": "~2.5",
        "phpunit/phpunit": "~5"
    },
    "autoload": {
        "psr-4": { "SerendipityHQ\\Bundle\\AwsSesMonitorBundle\\": "" }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
